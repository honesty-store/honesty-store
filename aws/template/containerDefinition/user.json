[
    {
        "name": "container",
        "image": "${image}",
        "memory": 64,
        "essential": true,
        "portMappings": [
            {
                "containerPort": 3000,
                "protocol": "tcp"
            }
        ],
        "environment": [
            {
                "name": "TABLE_NAME",
                "value": "${tableName}"
            },
            {
                "name": "AWS_REGION",
                "value": "eu-west-1"
            },
            {
                "name": "JWT_SECRET",
                "value": "0790b9b8-82c1-44da-8cce-fe46f9a496cb"
            }
        ]
    }
]